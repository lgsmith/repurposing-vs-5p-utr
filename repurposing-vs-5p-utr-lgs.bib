  langid = {english},
  keywords = {Alternative splicing,Transcriptomics},
  file = {/home/louis/Zotero/storage/PKICN3UW/Van Nostrand et al. - 2020 - A large-scale binding and functional map of human RNA-binding proteins.pdf}
}

