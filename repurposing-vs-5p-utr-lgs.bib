@article{hargrove2022,
  title = {R-{{BIND}} 2.0: {{An Updated Database}} of {{Bioactive RNA-Targeting Small Molecules}} and {{Associated RNA Secondary Structures}}},
  shorttitle = {R-{{BIND}} 2.0},
  author = {Donlic, Anita and Swanson, Emily G. and Chiu, Liang-Yuan and Wicks, Sarah L. and Juru, Aline Umuhire and Cai, Zhengguo and Kassam, Kamillah and Laudeman, Chris and Sanaba, Bilva G. and Sugarman, Andrew and Han, Eunseong and Tolbert, Blanton S. and Hargrove, Amanda E.},
