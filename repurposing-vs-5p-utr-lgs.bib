@article{dominguez2025,
  title = {Dissecting {{RNA}} Selectivity Mediated by Tandem {{RNA-binding}} Domains},
  author = {Harris, Sarah E. and Hu, Yue and Bridges, Kaitlin and Cavazos, Francisco F. and Martyr, Justin G. and Guzm{\'a}n, Bryan B. and Murn, Jernej and Aleman, Maria M. and Dominguez, Daniel},
