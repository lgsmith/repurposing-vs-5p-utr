@article{hargrove2021,
  title = {Amilorides Inhibit {{SARS-CoV-2}} Replication in Vitro by Targeting {{RNA}} Structures},
  author = {Zafferani, Martina and Haddad, Christina and Luo, Le and {Davila-Calderon}, Jesse and Chiu, Liang-Yuan and Mugisha, Christian Shema and Monaghan, Adeline G. and Kennedy, Andrew A. and Yesselman, Joseph D. and Gifford, Robert J. and Tai, Andrew W. and Kutluay, Sebla B. and Li, Mei-Ling and Brewer, Gary and Tolbert, Blanton S. and Hargrove, Amanda E.},
